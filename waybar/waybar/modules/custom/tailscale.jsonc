{
    "custom/tailscale": {
        "format": "{}",
        "escape": false,
        "return-type": "json",
        "exec": "~/.config/waybar/scripts/tailscaleinfo.sh",
        "interval": 60,
        "on-click": "~/.config/waybar/scripts/connectssh.sh",
        "on-click-right": "kitty --title 'SSH Connect' sh -c ~/.config/waybar/scripts/connectssh.sh",
        "tooltip": true
    }
}
